      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.linear_model import LinearRegression\n",
        "\n",
        "# STEP 2  Read dataset & clean missing values\n",
        "df = pd.read_csv(\"/content/sample_data/student_marks.csv\")\n",
        "\n",
        "print(\"Original Data:\")\n",
        "print(df)\n",
        "\n",
        "df[\"Marks\"] = df[\"Marks\"].fillna(df[\"Marks\"].mean())\n",
        "\n",
        "print(\"\\nAfter Cleaning:\")\n",
        "print(df)\n",
        "\n",
        "# STEP 3  Plot marks distribution\n",
        "plt.hist(df[\"Marks\"], bins=5)\n",
        "plt.xlabel(\"Marks\")\n",
        "plt.ylabel(\"Number of Students\")\n",
        "plt.title(\"Marks Distribution\")\n",
        "plt.show()\n",
        "\n",
        "# STEP 4  Linear Regression Model\n",
        "X = df[[\"Hours_Studied\"]]\n",
        "y = df[\"Marks\"]\n",
        "\n",
        "model = LinearRegression()\n",
        "model.fit(X, y)\n",
        "\n",
        "predicted = model.predict([[7.5]])\n",
        "print(\"Predicted Marks for 7.5 hours studied:\", predicted[0])\n",
        "\n",
        "# STEP 5  Regression line visualization\n",
        "plt.scatter(X, y, label=\"Actual Data\")\n",
        "plt.plot(X, model.predict(X), label=\"Regression Line\")\n",
        "plt.xlabel(\"Hours Studied\")\n",
        "plt.ylabel(\"Marks\")\n",
        "plt.title(\"Hours vs Marks\")\n",
        "plt.legend()\n",
        "plt.show()\n"
      ]
