        "# 1. Read a CSV file\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "print(df)\n",
        "\n",
        "\n",
        "# 2. Display first and last 5 rows\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "print(df.head())\n",
        "print(df.tail())\n",
        "\n",
        "\n",
        "# 3. Check rows, columns and data types\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "print(df.shape)      # rows, columns\n",
        "print(df.columns)\n",
        "print(df.dtypes)\n",
        "\n",
        "\n",
        "# 4. Select a single column\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "print(df[\"Age\"])\n",
        "\n",
        "\n",
        "# 5. Select multiple columns\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "print(df[[\"Name\",\"Salary\"]])\n",
        "\n",
        "\n",
        "# 6. Filter data (Age > 30)\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "filtered = df[df[\"Age\"] > 30]\n",
        "print(filtered)\n",
        "\n",
        "\n",
        "# 7. Find missing values\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "print(df.isnull())\n",
        "print(df.isnull().sum())\n",
        "\n",
        "\n",
        "# 8. Fill missing values\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "df[\"Age\"].fillna(df[\"Age\"].mean(), inplace=True)\n",
        "print(df)\n",
        "\n",
        "\n",
        "# 9. Remove duplicate rows\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "df = df.drop_duplicates()\n",
        "print(df)\n",
        "\n",
        "\n",
        "# 10. Sort data by Salary\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "df = df.sort_values(\"Salary\")\n",
        "print(df)\n",
        "\n",
        "\n",
        "# 11. Calculate average salary\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "print(\"Average Salary:\", df[\"Salary\"].mean())\n",
        "\n",
        "\n",
        "# 12. Group by department\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "print(df.groupby(\"Department\")[\"Salary\"].mean())\n",
        "\n",
        "\n",
        "# 13. Add a new column\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "df[\"Bonus\"] = df[\"Salary\"] * 0.10\n",
        "print(df)\n",
        "\n",
        "\n",
        "# 14. Save to new CSV\n",
        "import pandas as pd\n",
        "df = pd.read_csv(\"/content/sample_data/bio.csv\")\n",
        "df.to_csv(\"newdata.csv\", index=False)\n",
        "\n",
        "\n",
        "# 15. Simple Student Marks Analysis\n",
        "import pandas as pd\n",
        "data = {\n",
        "    \"Name\": [\"Asha\",\"Bala\",\"Ravi\",\"Meena\"],\n",
        "    \"Marks\": [85, 90, 78, 92]\n",
        "}\n",
        "df = pd.DataFrame(data)\n",
        "print(df)\n",
        "print(\"Highest Marks:\", df[\"Marks\"].max())\n",
        "print(\"Average Marks:\", df[\"Marks\"].mean())\n"
